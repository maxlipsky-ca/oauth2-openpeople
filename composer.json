{
    "name": "maxlipsky-ca/oauth2-openpeople",
    "description": "Open People Search OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Lipsky",
            "email": "maxlipsky@gmail.com",
            "homepage": "https://github.com/maxlipsky-ca"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "openpeople",
        "openpeoplesearch"
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "mockery/mockery": "~1.4",
        "squizlabs/php_codesniffer": "~3.6",
        "ext-json": "*",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "MaxLipsky\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaxLipsky\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
